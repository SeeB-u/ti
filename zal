#include <stdio.h>
#include <math.h>
int main() {
int n=3;
double wyraz,x;
printf("Podaj kat w radianach: ");
scanf("%lf",&x);
wyraz=x;
do {
wyraz=wyraz*(-(x*x)/((n-1)*n));
n=n+2;
}
while(n<=20);
printf("\n\n %lf,", wyraz);
getchar();
printf("\n\n\n\n\n\n\n sin (%lf) wynosi %lf", x, sin(x));
getchar();
return 0;
}
